msgctxt ""
"03132000.xhp\n"
"par_id3143228\n"
"help.text"
msgid "oListener = CreateUnoListener( Prefixname, ListenerInterfaceName )"
msgstr "oListener = CreateUnoListener( Prefixname, ListenerInterfaceName )"

