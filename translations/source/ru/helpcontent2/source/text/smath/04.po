msgid "Formula Shortcut Keys"
msgstr "Сочетания клавиш для работы с формулами"

#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"bm_id3154702\n"
"help.text"
msgid "<bookmark_value>shortcut keys; in formulas</bookmark_value>"
msgstr "<bookmark_value>сочетания клавиш; в формулах</bookmark_value>"

