msgstr "<ahelp hid=\".\">选择组合条件的字段名称。</ahelp>"

#: querywizard06.xhp
msgctxt ""
"querywizard06.xhp\n"
"par_idN1056E\n"
"help.text"
msgid "Condition"
msgstr "条件"

#: querywizard06.xhp
msgctxt ""
"querywizard06.xhp\n"
"par_idN10572\n"
"help.text"
msgid "<ahelp hid=\".\">Select the condition for the grouping.</ahelp>"
msgstr "<ahelp hid=\".\">选择组合条件。</ahelp>"

#: querywizard06.xhp
msgctxt ""
"querywizard06.xhp\n"
"par_idN10575\n"
"help.text"
msgid "Value"
msgstr "值"

#: querywizard06.xhp
msgctxt ""
"querywizard06.xhp\n"
"par_idN10579\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the value for the grouping condition.</ahelp>"
msgstr "<ahelp hid=\".\">输入组合条件的值。</ahelp>"

#: querywizard06.xhp
msgctxt ""
"querywizard06.xhp\n"
"par_idN1057C\n"
