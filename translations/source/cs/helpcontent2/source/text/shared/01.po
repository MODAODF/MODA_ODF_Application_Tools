#: 06050300.xhp
msgctxt ""
"06050300.xhp\n"
"tit\n"
"help.text"
msgid "Outline"
msgstr "Osnova"

#: 06050300.xhp
msgctxt ""
"06050300.xhp\n"
"hd_id3147543\n"
"help.text"
