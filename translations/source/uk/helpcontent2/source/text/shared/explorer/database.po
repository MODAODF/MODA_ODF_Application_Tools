msgid "<image src=\"media/icon-themes/cmd/sc_sortascending.svg\" id=\"img_id631540564204841\" width=\"1.0cm\" height=\"1.0cm\"><alt id=\"alt_id141540564204841\">Sort Ascending icon</alt></image>"
msgstr "<image src=\"media/icon-themes/cmd/sc_sortascending.svg\" id=\"img_id631540564204841\" width=\"1.0cm\" height=\"1.0cm\"><alt id=\"alt_id141540564204841\">Піктограма Упорядкувати за зростанням</alt></image>"

#: rep_insertfield.xhp
