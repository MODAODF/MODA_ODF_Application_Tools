msgctxt ""
"03080102.xhp\n"
"par_id3144764\n"
"help.text"
msgid "d1 = InputBox(\"Enter the length of the adjacent side: \",\"Adjacent\")"
msgstr "d1 = InputBox(\"Introduza o comprimento do lado adjacente: \",\"Adjacente\")"

