#: forms/inc/strings.hrc:77
msgctxt "RID_STR_DATATYPE_YEAR"
msgid "Year"
msgstr "ឆ្នាំ"

#: forms/inc/strings.hrc:78
msgctxt "RID_STR_DATATYPE_MONTH"
