msgstr "Nový text"

#: sc/inc/scfuncs.hrc:3740
msgctxt "SC_OPCODE_REPLACE"
msgid "The text to be inserted."
msgstr "Text, ktorý sa má vložiť."

#: sc/inc/scfuncs.hrc:3746
msgctxt "SC_OPCODE_FIXED"
