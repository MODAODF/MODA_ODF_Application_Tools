msgctxt ""
"03100100.xhp\n"
"par_id3149514\n"
"help.text"
msgid "CBool (Expression1 {= | <> | < | > | <= | >=} Expression2) or CBool (Number)"
msgstr "CBool (Expression1 {= | <> | < | > | <= | >=} Expression2) or CBool (Number)"

#: 03100100.xhp
msgctxt ""
"03100100.xhp\n"
"hd_id3156152\n"
"help.text"
msgid "Return value:"
msgstr "返回值:"

