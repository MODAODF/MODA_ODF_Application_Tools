#: sc/inc/scfuncs.hrc:3553
msgctxt "SC_OPCODE_CHAR"
msgid "Number"
msgstr "Tala"

#: sc/inc/scfuncs.hrc:3554
msgctxt "SC_OPCODE_CHAR"
