"03030206.xhp\n"
"par_id3149378\n"
"help.text"
msgid "a1 = \"start time\""
msgstr "a1 = \"algusaeg\""

#: 03030206.xhp
msgctxt ""
"03030206.xhp\n"
"par_id3145800\n"
