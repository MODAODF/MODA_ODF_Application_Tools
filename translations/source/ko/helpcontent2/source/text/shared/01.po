#: 02040000.xhp
msgctxt ""
"02040000.xhp\n"
"par_id3153255\n"
"help.text"
msgid "<ahelp hid=\".uno:Cut\">Removes and copies the selection to the clipboard.</ahelp>"
msgstr "<ahelp hid=\".uno:Cut\">선택 사항을 클립보드에서 제거하고 복사합니다.</ahelp>"

