#: forms/inc/strings.hrc:75
msgctxt "RID_STR_DATATYPE_TIME"
msgid "Time"
msgstr "ពេល​វេលា​"

#: forms/inc/strings.hrc:76
msgctxt "RID_STR_DATATYPE_DATETIME"
msgid "Date and Time"
