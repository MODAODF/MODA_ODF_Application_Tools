msgctxt ""
"03090303.xhp\n"
"par_id3150321\n"
"help.text"
msgid "sVar =sVar & \" Label 1\" : GoTo Ende"
msgstr "sVar =sVar & \" Návěstí 1\" : GoTo Ende"

#: 03090303.xhp
msgctxt ""
"03090303.xhp\n"
"par_id3155764\n"
"help.text"
