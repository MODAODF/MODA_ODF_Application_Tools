msgctxt ""
"tablewizard02.xhp\n"
"par_idN10574\n"
"help.text"
msgid "-"
msgstr "-"

#: tablewizard02.xhp
msgctxt ""
"tablewizard02.xhp\n"
"par_idN10578\n"
"help.text"
msgid "<ahelp hid=\".\">Remove the selected field from the list box.</ahelp>"
msgstr "<ahelp hid=\".\">Kendu hautatutako eremua zerrenda-koadrotik.</ahelp>"

