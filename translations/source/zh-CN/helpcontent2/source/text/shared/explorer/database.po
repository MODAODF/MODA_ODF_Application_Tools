msgctxt ""
"querywizard03.xhp\n"
"par_idN105E6\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the value for the filter condition.</ahelp>"
msgstr "<ahelp hid=\".\">输入筛选条件的值。</ahelp>"

#: querywizard03.xhp
msgctxt ""
"querywizard03.xhp\n"
"par_idN10574\n"
"help.text"
