msgctxt ""
"02230100.xhp\n"
"par_id3153252\n"
"help.text"
msgid "Insert AutoText."
msgstr "Indsæt autotekst."

#: 02230100.xhp
msgctxt ""
"02230100.xhp\n"
"par_id3155449\n"
"help.text"
msgid "Insert from clipboard."
msgstr "Indsæt fra udklipsholder."

#: 02230100.xhp
msgctxt ""
"02230100.xhp\n"
"par_id3153821\n"
"help.text"
msgid "Change cell contents by insertions and deletions."
msgstr "Skift celleindhold med indsættelser og sletninger"

#: 02230100.xhp
msgctxt ""
"02230100.xhp\n"
"par_id3150771\n"
"help.text"
msgid "Insert or delete columns and rows."
msgstr "Indsæt eller slet kolonner og rækker"

