msgctxt ""
"06040100.xhp\n"
"par_id8703573\n"
"help.text"
msgid "A--B (A, minus, minus, B)"
msgstr "A -- B (A, väli, miinus, miinus, väli, B)"

#: 06040100.xhp
msgctxt ""
"06040100.xhp\n"
"par_id6049684\n"
"help.text"
msgid "A—B (A, em-dash, B)<br/>(see note below the table)"
msgstr "A—B (A, pitkä ajatusviiva, B)<br/>(katso huomautusta taulukon alla)"

