msgstr "<ahelp hid=\"cui/ui/transparencytabpage/MTR_TRGR_CENTER_X\"> Introduza o desprazamento ao gradiente horizontal.</ahelp>"

#: 05210700.xhp
msgctxt ""
"05210700.xhp\n"
"hd_id3154897\n"
"help.text"
msgid "Center Y"
msgstr "Centrar Y"

