msgid "PEARSON"
msgstr "PEARSON"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3153216\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_PEARSON\">Returns the Pearson product moment correlation coefficient r.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_PEARSON\">Vrátí Pearsonův korelační koeficient r.</ahelp>"

