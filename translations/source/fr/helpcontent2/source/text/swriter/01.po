msgid "<ahelp hid=\".\">Specify the document that you want to use as a base for the mail merge document.</ahelp>"
msgstr "<ahelp hid=\".\">Spécifiez le document à utiliser comme base pour le document de mailing.</ahelp>"

#: mailmerge01.xhp
msgctxt ""
"mailmerge01.xhp\n"
"par_idN10556\n"
"help.text"
msgid "Use the current document"
msgstr "Utiliser le document actif"

