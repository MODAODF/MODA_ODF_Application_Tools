msgctxt ""
"text_color.xhp\n"
"par_id3154285\n"
"help.text"
msgid "<link href=\"text/shared/01/05020100.xhp\" name=\"Font color\">Font color</link>"
msgstr "<link href=\"text/shared/01/05020100.xhp\" name=\"Цвет шрифта\">Цвет шрифта</link>"

