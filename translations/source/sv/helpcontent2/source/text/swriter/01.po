msgctxt ""
"02120000.xhp\n"
"par_id3145173\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/editcategories/EditCategoriesDialog\">Adds, renames, or deletes AutoText categories.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/editcategories/EditCategoriesDialog\">Lägger till, byter namn på eller tar bort autotext-områden.</ahelp>"

