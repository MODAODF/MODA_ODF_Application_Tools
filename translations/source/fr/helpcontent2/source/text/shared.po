#: 3dsettings_toolbar.xhp
msgctxt ""
"3dsettings_toolbar.xhp\n"
"par_idN10667\n"
"help.text"
msgid "<ahelp hid=\".\">Select a direction.</ahelp>"
msgstr "<ahelp hid=\".\">Sélectionnez une orientation.</ahelp>"

#: 3dsettings_toolbar.xhp
msgctxt ""
"3dsettings_toolbar.xhp\n"
"par_idN10698\n"
"help.text"
