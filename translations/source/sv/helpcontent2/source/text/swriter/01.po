#: 05060100.xhp
msgctxt ""
"05060100.xhp\n"
"par_id3145413\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/frmtypepage/relwidth\">Calculates the width of the selected object as a percentage of the width of the page text area.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/frmtypepage/relwidth\">Beräknar det markerade objektets bredd som en procentandel av bredden på sidans textområde.</ahelp>"

