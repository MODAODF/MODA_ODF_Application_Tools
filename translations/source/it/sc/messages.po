#: sc/inc/scfuncs.hrc:3511
msgctxt "SC_OPCODE_BAHTTEXT"
msgid "Number"
msgstr "Numero"

#: sc/inc/scfuncs.hrc:3512
msgctxt "SC_OPCODE_BAHTTEXT"
msgid "The number to convert."
msgstr "Il numero da convertire."

