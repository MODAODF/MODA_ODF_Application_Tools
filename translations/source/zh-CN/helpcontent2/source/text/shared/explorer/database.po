msgctxt ""
"querywizard04.xhp\n"
"par_idN1060B\n"
"help.text"
msgid "<ahelp hid=\".\">Removes the last row of controls.</ahelp>"
msgstr "<ahelp hid=\".\">删除最后一行控件。</ahelp>"

