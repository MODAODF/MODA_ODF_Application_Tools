msgstr "YannateAante (yanna, daqiiqa, sekonde)"

#: 03030205.xhp
msgctxt ""
"03030205.xhp\n"
"hd_id3148797\n"
"help.text"
msgid "Return value:"
msgstr "Qolote guma:"

#: 03030205.xhp
msgctxt ""
"03030205.xhp\n"
"par_id3154908\n"
"help.text"
msgid "Date"
msgstr "Barra"

