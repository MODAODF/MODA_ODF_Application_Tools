msgctxt ""
"05200200.xhp\n"
"par_id3154749\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/linestyletabpage/BTN_LOAD\">Imports a list of line styles.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/linestyletabpage/BTN_LOAD\">Tuodaan viivatyyliluettelo.</ahelp>"

#: 05200200.xhp
msgctxt ""
"05200200.xhp\n"
"hd_id3148642\n"
"help.text"
msgid "Save line style table"
msgstr "Tallennetaan viivatyylitaulukko"

#: 05200200.xhp
msgctxt ""
"05200200.xhp\n"
"par_id3155449\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/linestyletabpage/BTN_SAVE\">Saves the current list of line styles, so that you can load it again later.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/linestyletabpage/BTN_SAVE\">Tallennetaan nykyinen viivatyyliluettelo, niin että sen voi ladata myöhemmin.</ahelp>"

