msgid "<bookmark_value>ISTEXT function</bookmark_value>  <bookmark_value>cell contents;text</bookmark_value>"
msgstr "<bookmark_value>hàm ISTEXT</bookmark_value><bookmark_value>nội dung ô;văn bản</bookmark_value>"

#: 04060104.xhp
msgctxt ""
"04060104.xhp\n"
"hd_id3149426\n"
"help.text"
msgid "ISTEXT"
msgstr "ISTEXT"

