msgctxt ""
"menuedit.xhp\n"
"par_idN105E9\n"
"help.text"
msgid "<ahelp hid=\".\">Opens the Database Properties dialog.</ahelp>"
msgstr "<ahelp hid=\".\">打开「数据库属性」对话框。</ahelp>"

