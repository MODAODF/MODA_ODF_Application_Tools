msgid "<image id=\"img_id3148420\" src=\"sw/res/sc20173.png\" width=\"0.2228in\" height=\"0.2228in\"><alt id=\"alt_id3148420\">Icon</alt></image>"
msgstr "<image id=\"img_id3148420\" src=\"sw/res/sc20173.png\" width=\"0.2228in\" height=\"0.2228in\"><alt id=\"alt_id3148420\">图标</alt></image>"

#: 02110000.xhp
