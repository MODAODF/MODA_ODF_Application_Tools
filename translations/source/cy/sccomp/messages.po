#: sccomp/inc/strings.hrc:25
msgctxt "RID_SOLVER_COMPONENT"
msgid "%PRODUCTNAME Linear Solver"
msgstr "Datrysydd Llinellol %PRODUCTNAME"

#: sccomp/inc/strings.hrc:26
msgctxt "RID_COINMP_SOLVER_COMPONENT"
msgid "%PRODUCTNAME CoinMP Linear Solver"
msgstr "Datrysydd Llinellol CoinMP %PRODUCTNAME"

#: sccomp/inc/strings.hrc:27
msgctxt "RID_SWARM_SOLVER_COMPONENT"
msgid "%PRODUCTNAME Swarm Non-Linear Solver (experimental)"
msgstr "Datrysydd an Llinynnol Swarm %PRODUCTNAME (arbrofol)"

#: sccomp/inc/strings.hrc:28
msgctxt "RID_PROPERTY_NONNEGATIVE"
msgid "Assume variables as non-negative"
msgstr "Tybio fod yr amrywiolion yn annegyddol"

#: sccomp/inc/strings.hrc:29
msgctxt "RID_PROPERTY_INTEGER"
msgid "Assume variables as integer"
msgstr "Tybio amrywiolion fel cyfanrif"

#: sccomp/inc/strings.hrc:30
msgctxt "RID_PROPERTY_TIMEOUT"
msgid "Solving time limit (seconds)"
msgstr "Datrys terfyn amser (eiliad)"

#: sccomp/inc/strings.hrc:31
msgctxt "RID_PROPERTY_EPSILONLEVEL"
msgid "Epsilon level (0-3)"
msgstr "Lefel epsilon (0-3)"

#: sccomp/inc/strings.hrc:32
msgctxt "RID_PROPERTY_LIMITBBDEPTH"
msgid "Limit branch-and-bound depth"
msgstr "Cyfyngu ar derfyn rannu a rhwymo"

#: sccomp/inc/strings.hrc:33
msgctxt "RID_PROPERTY_ALGORITHM"
msgid "Swarm algorithm (0 - Differential Evolution, 1 - Particle Swarm Optimization)"
msgstr "Algorithm Swarm (0 - Esblygiad Gwahaniaethol, 1 - Gwella Gronynnau Swarm)"

#: sccomp/inc/strings.hrc:34
msgctxt "RID_ERROR_NONLINEAR"
msgid "The model is not linear."
msgstr "Nid yw'r model yn llinellol."

#: sccomp/inc/strings.hrc:35
msgctxt "RID_ERROR_EPSILONLEVEL"
msgid "The epsilon level is invalid."
msgstr "Mae'r lefel epsilon yn annilys."

#: sccomp/inc/strings.hrc:36
msgctxt "RID_ERROR_INFEASIBLE"
msgid "The model is infeasible. Check limiting conditions."
msgstr "Mae'r model yn annichonadwy. Gwirio cyflyrau cyfyngu."

#: sccomp/inc/strings.hrc:37
msgctxt "RID_ERROR_UNBOUNDED"
msgid "The model is unbounded."
msgstr "Mae'r model yn ddiderfyn."

#: sccomp/inc/strings.hrc:38
msgctxt "RID_ERROR_TIMEOUT"
msgid "The time limit was reached."
msgstr "Wedi cyrraedd terfyn amser."
