msgctxt ""
"05230300.xhp\n"
"hd_id3154173\n"
"help.text"
msgid "Angle"
msgstr "Úhel"

#: 05230300.xhp
msgctxt ""
"05230300.xhp\n"
"par_id3147573\n"
"help.text"
