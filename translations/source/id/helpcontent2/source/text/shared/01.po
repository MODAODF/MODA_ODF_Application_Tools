"help.text"
msgid "<link href=\"text/shared/01/05080100.xhp\" name=\"Left\">Left</link>"
msgstr "<link href=\"text/shared/01/05080100.xhp\" name=\"Left\">Kiri</link>"

#: 05080100.xhp
msgctxt ""
"05080100.xhp\n"
"par_id3150756\n"
