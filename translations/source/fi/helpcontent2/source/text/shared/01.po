msgctxt ""
"06040100.xhp\n"
"par_id1868037\n"
"help.text"
msgid "A-B (unchanged)"
msgstr "A-B (muuttumaton)"

#: 06040100.xhp
msgctxt ""
"06040100.xhp\n"
"par_id714438\n"
"help.text"
msgid "A -B (A, space, minus, B)"
msgstr "A -B (A, väli, miinus, B)"

