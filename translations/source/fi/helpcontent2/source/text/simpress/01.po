msgctxt ""
"04030000.xhp\n"
"hd_id3147366\n"
"help.text"
msgid "Point"
msgstr "Piste"

#: 04030000.xhp
msgctxt ""
"04030000.xhp\n"
"par_id3155926\n"
"help.text"
msgid "<ahelp hid=\"modules/sdraw/ui/dlgsnap/point\">Inserts a snap point.</ahelp>"
msgstr "<ahelp hid=\"modules/sdraw/ui/dlgsnap/point\">Lisätään kohdistuspiste.</ahelp>"

