#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"OKLabel\n"
"LngText.text"
msgid "Ok"
msgstr "OK"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallLabel\n"
"LngText.text"
msgid "Install"
msgstr "Installeren"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AbortLabel\n"
"LngText.text"
msgid "Abort"
msgstr "Afbreken"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText1\n"
"LngText.text"
msgid "Welcome to the [FULLPRODUCTNAME] Installation Wizard"
msgstr "Welkom bij de [FULLPRODUCTNAME] Installatie-assistent"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText2\n"
"LngText.text"
msgid "This installation will update your installed versions of [PRODUCTNAME]"
msgstr "Deze installatie zal uw ge√Ønstalleerde versies van [PRODUCTNAME] bijwerken"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText3\n"
"LngText.text"
msgid "This might take a moment."
msgstr "Dit kan enige tijd vergen."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseMyOwnText\n"
"LngText.text"
msgid "Not listed (choose location in an extra step)"
msgstr "Niet weergegeven (kies een locatie in een extra stap)"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListPromptText\n"
"LngText.text"
msgid "Choose [PRODUCTNAME] [PRODUCTVERSION] installation for which you want to install the [FULLPRODUCTNAME]"
msgstr "Kies installatie van [PRODUCTNAME] [PRODUCTVERSION] waarvoor u de [FULLPRODUCTNAME] wilt installeren"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseManualText\n"
"LngText.text"
msgid "Point the dialog to your [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "Wijs in het dialoogvenster uw installatie van [PRODUCTNAME] [PRODUCTVERSION] aan."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListOKLabelText\n"
"LngText.text"
msgid "Install"
msgstr "Installeren"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListCancelLabel\n"
"LngText.text"
msgid "Abort"
msgstr "Afbreken"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText1\n"
"LngText.text"
msgid "This is not a valid [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "Dit is geen geldige installatie van [PRODUCTNAME] [PRODUCTVERSION]."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText2\n"
"LngText.text"
msgid "Run the installer again and choose a valid [PRODUCTNAME] [PRODUCTVERSION] installation"
msgstr "Voer de installatie opnieuw uit en kies een geldige installatie van [PRODUCTNAME] [PRODUCTVERSION]"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText1\n"
"LngText.text"
msgid "Click Install to start the installation"
msgstr "Klik op Installeren om de installatie te beginnen"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText2\n"
"LngText.text"
msgid "Installation might take a minute..."
msgstr "Installatie kan enige tijd vergen..."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText\n"
"LngText.text"
msgid "Installation failed; most likely your account does not have the necessary privileges."
msgstr "Installatie mislukt; waarschijnlijk heeft uw account niet de benodigde rechten."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText2\n"
"LngText.text"
msgid "Do you want to identify as administrator and try again?"
msgstr "Wilt u zich aanmelden als beheerder en opnieuw proberen?"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyYES\n"
"LngText.text"
msgid "Yes, identify"
msgstr "Ja, aanmelden"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyNO\n"
"LngText.text"
msgid "No, abort installation"
msgstr "Nee, installatie afbreken"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallFailedText\n"
"LngText.text"
msgid "Installation failed."
msgstr "Installatie mislukt."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteText\n"
"LngText.text"
msgid "Installation of [PRODUCTNAME] language pack completed."
msgstr "Installatie van taalpakket voor [PRODUCTNAME] voltooid."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteText2\n"
"LngText.text"
msgid "Call '[PRODUCTNAME]-Preferences-Language Settings-Languages' to change the user interface language."
msgstr "Ga naar '[PRODUCTNAME]-Opties-Taalinstellingen-Talen' om de taal van de gebruikersinterface te wijzigen."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteTextPatch\n"
"LngText.text"
msgid "Installation of [FULLPRODUCTNAME] completed"
msgstr "Installatie van [FULLPRODUCTNAME] voltooid"
