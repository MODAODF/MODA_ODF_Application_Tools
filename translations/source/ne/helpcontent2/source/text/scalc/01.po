"04060184.xhp\n"
"par_id3149286\n"
"help.text"
msgid "<item type=\"input\">=PHI(0)</item> = 0.4"
msgstr "<item type=\"input\">=PHI(0)</item> = ०.४"

#: 04060184.xhp
