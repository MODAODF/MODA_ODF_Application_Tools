"03020405.xhp\n"
"par_id3148406\n"
"help.text"
msgid "8 - APPEND (file open for appending)"
msgstr "8 - APPEND (fail on lisamiseks avatud)"

#: 03020405.xhp
msgctxt ""
"03020405.xhp\n"
"par_id3154757\n"
"help.text"
