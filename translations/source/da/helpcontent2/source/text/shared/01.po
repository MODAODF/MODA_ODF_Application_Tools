msgctxt ""
"05230400.xhp\n"
"hd_id3154497\n"
"help.text"
msgid "Corner Radius"
msgstr "Hjørneradius"

#: 05230400.xhp
msgctxt ""
"05230400.xhp\n"
"par_id3156027\n"
"help.text"
msgid "You can only round the corners of a rectangular object."
msgstr "Du kan kun afrunde hjørnerne af et rektangulært objekt."

