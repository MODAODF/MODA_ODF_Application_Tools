msgctxt ""
"03090101.xhp\n"
"par_id3146912\n"
"help.text"
msgid "MsgBox \"The expiration date has not yet passed\""
msgstr "MsgBox \"Trvanlivost ještě nevypršela\""

#: 03090101.xhp
msgctxt ""
"03090101.xhp\n"
"par_id3154754\n"
"help.text"
msgid "MsgBox \"The expiration date is today\""
msgstr "MsgBox \"Datum trvanlivosti je dnes\""

