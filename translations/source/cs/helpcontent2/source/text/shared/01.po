#: 05120300.xhp
msgctxt ""
"05120300.xhp\n"
"par_id3149783\n"
"help.text"
msgid "<ahelp hid=\".uno:SpacePara2\">Sets the line spacing of the current paragraph to two lines.</ahelp>"
msgstr "<ahelp hid=\".uno:SpacePara2\">Nastaví pro aktuální odstavec dvojité řádkování. </ahelp>"

