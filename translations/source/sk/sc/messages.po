msgctxt "SC_OPCODE_TEXT"
msgid "Format"
msgstr "Formát"

#: sc/inc/scfuncs.hrc:3718
msgctxt "SC_OPCODE_TEXT"
msgid "The text that describes the format."
msgstr "Text ktorý popisuje výsledný formát."

#: sc/inc/scfuncs.hrc:3724
msgctxt "SC_OPCODE_T"
