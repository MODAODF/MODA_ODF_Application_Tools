msgctxt ""
"04060110.xhp\n"
"par_id3153938\n"
"help.text"
msgid "<item type=\"input\">=ROMAN(999;0)</item> returns CMXCIX"
msgstr "<item type=\"input\">=ROMAN(999;0)</item> kthen CMXCIX"

