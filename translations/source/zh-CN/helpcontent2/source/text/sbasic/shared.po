msgctxt ""
"03090404.xhp\n"
"hd_id3150771\n"
"help.text"
msgid "<link href=\"text/sbasic/shared/03090404.xhp\" name=\"End Statement\">End Statement</link>"
msgstr "<link href=\"text/sbasic/shared/03090404.xhp\" name=\"End Statement\">End 语句</link>"

