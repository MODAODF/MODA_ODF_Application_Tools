msgstr ""

#: xml_source.xhp
msgctxt ""
"xml_source.xhp\n"
"par_id291521494762812\n"
"help.text"
msgid "The dialog consists of four parts."
msgstr ""

#: xml_source.xhp
msgctxt ""
