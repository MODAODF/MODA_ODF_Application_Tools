msgstr "<variable id=\"loeschentext\"><ahelp hid=\".uno:DeleteColumns\">Usuwa zaznaczone kolumny z tabeli.</ahelp></variable>"

#: 05120500.xhp
msgctxt ""
"05120500.xhp\n"
"par_id3156385\n"
"help.text"
msgid "This command is only available if the cursor is in a table."
