#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"bm_id4150019\n"
"help.text"
msgid "<bookmark_value>BITXOR function</bookmark_value>"
msgstr "<bookmark_value>hàm OR</bookmark_value>"

#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"hd_id4150019\n"
"help.text"
msgid "BITXOR"
msgstr ""

#: 04060120.xhp
