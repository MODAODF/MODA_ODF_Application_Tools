msgctxt ""
"querywizard03.xhp\n"
"par_idN105CC\n"
"help.text"
msgid "<ahelp hid=\".\">Select the field name for the filter condition.</ahelp>"
msgstr "<ahelp hid=\".\">选择筛选条件的字段名称。</ahelp>"

#: querywizard03.xhp
msgctxt ""
"querywizard03.xhp\n"
"par_idN10568\n"
"help.text"
