msgctxt ""
"05340400.xhp\n"
"par_id3153700\n"
"help.text"
msgid "Click the row header"
msgstr "klik garis tajuk"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3149575\n"
"help.text"
msgid "Several records or removing a selection"
msgstr "Beberapa catatan atau menghapus pilihan"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3149295\n"
"help.text"
msgid "Hold down <switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline> and click the row header"
msgstr "Menekan <switchinline select=\"sys\"><caseinline select=\"MAC\">Perintah</caseinline><defaultinline>Ctlrl</defaultinline></switchinline>dan klik garis tajuk"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3152360\n"
"help.text"
msgid "Column"
msgstr "Kolom"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3153960\n"
"help.text"
msgid "Click the column header"
msgstr "Klik kolom tajuk"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3150541\n"
"help.text"
msgid "Data field"
msgstr "Bidang data"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3150358\n"
"help.text"
msgid "Click in the data field"
msgstr "klik dalam ruas data"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3154366\n"
"help.text"
msgid "Entire table"
msgstr "Seluruh Halaman"

