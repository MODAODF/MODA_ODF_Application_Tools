msgctxt ""
"03140012.xhp\n"
"par_id240720170144221476\n"
"help.text"
msgid "print syd_yr1 ' syd_yr1 is now equal to 3000."
msgstr "print syd_yr1 ' syd_yr1 agora é igual a 3000."

