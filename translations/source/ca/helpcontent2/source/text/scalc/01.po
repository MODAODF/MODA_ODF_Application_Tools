msgstr ""

#: live_data_stream.xhp
msgctxt ""
"live_data_stream.xhp\n"
"hd_id240920171003006302\n"
"help.text"
msgid "<link href=\"text/scalc/01/live_data_stream.xhp\">Live Data Stream</link>"
msgstr ""

#: live_data_stream.xhp
msgctxt ""
"live_data_stream.xhp\n"
