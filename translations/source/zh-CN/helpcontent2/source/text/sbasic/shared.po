msgctxt ""
"03090404.xhp\n"
"tit\n"
"help.text"
msgid "End Statement"
msgstr "End 语句"

#: 03090404.xhp
msgctxt ""
"03090404.xhp\n"
"bm_id3150771\n"
"help.text"
msgid "<bookmark_value>End statement</bookmark_value>"
msgstr "<bookmark_value>End 语句</bookmark_value>"

