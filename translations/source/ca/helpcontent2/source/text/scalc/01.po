"par_id3152551\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_WERT\">Converts a text string into a number.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_WERT\">Converteix una cadena de text en un nombre.</ahelp>"

#: 04060110.xhp
msgctxt ""
