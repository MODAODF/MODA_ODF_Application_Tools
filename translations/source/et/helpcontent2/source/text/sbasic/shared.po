msgid "<ahelp hid=\".\">Specify the maximum value for the current control.</ahelp>"
msgstr "<ahelp hid=\".\">Määra aktiivse juhtelemendi maksimaalne väärtus.</ahelp>"

#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"hd_id3149276\n"
"help.text"
msgid "Value min."
msgstr "Min. suurus"

#: 01170101.xhp
msgctxt ""
