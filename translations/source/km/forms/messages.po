#: forms/inc/strings.hrc:73
msgctxt "RID_STR_DATATYPE_DOUBLE"
msgid "Double"
msgstr "ទ្វេ"

#: forms/inc/strings.hrc:74
msgctxt "RID_STR_DATATYPE_DATE"
msgid "Date"
