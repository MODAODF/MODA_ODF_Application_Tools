msgstr "<ahelp hid=\"HID_FUNC_GAUSS\">Tagastab kumulatiivse standardiseeritud normaaljaotuse.</ahelp>"

#: 04060182.xhp
msgctxt ""
"04060182.xhp\n"
"par_id2059694\n"
"help.text"
msgid "It is GAUSS(x)=NORMSDIST(x)-0.5"
msgstr "Selleks on GAUSS(x)=NORMSDIST(x)-0,5"

