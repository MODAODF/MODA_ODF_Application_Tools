msgctxt ""
"querywizard03.xhp\n"
"par_idN105D9\n"
"help.text"
msgid "<ahelp hid=\".\">Select the condition for the filter.</ahelp>"
msgstr "<ahelp hid=\".\">选择筛选条件。</ahelp>"

