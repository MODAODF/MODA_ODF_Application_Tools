msgctxt ""
"03020204.xhp\n"
"par_id3153711\n"
"help.text"
msgid "Put #iNumber,, \"This is the first line of text\" ' Fill line with text"
msgstr "Put #iNumber,, \"Det här är den första textraden\" ' Fyll raden med text"

#: 03020204.xhp
msgctxt ""
"03020204.xhp\n"
"par_id3155446\n"
"help.text"
msgid "Put #iNumber,, \"This is the second line of text\""
msgstr "Put #iNumber, \"Det här är den andra textraden\""

#: 03020204.xhp
msgctxt ""
"03020204.xhp\n"
"par_id3154255\n"
"help.text"
msgid "Put #iNumber,, \"This is the third line of text\""
msgstr "Put #iNumber, \"Det här är den tredje textraden\""

#: 03020204.xhp
msgctxt ""
"03020204.xhp\n"
"par_id3150940\n"
"help.text"
msgid "Put #iNumber,,\"This is new text\""
msgstr "Put #iNumber,,\"Det här är ny text\""

#: 03020204.xhp
msgctxt ""
