"rep_insertfield.xhp\n"
"par_id401540563942665\n"
"help.text"
msgid "Sort names descending,"
msgstr "Упорядковує назви за спаданням,"

#: rep_insertfield.xhp
msgctxt ""
"rep_insertfield.xhp\n"
"par_id851540564212907\n"
"help.text"
msgid "<image src=\"media/icon-themes/cmd/lc_removefiltersort.svg\" id=\"img_id41540564212907\" width=\"1.0cm\" height=\"1.0cm\"><alt id=\"alt_id421540564212908\">Undo sorting icon</alt></image>"
msgstr "<image src=\"media/icon-themes/cmd/lc_removefiltersort.svg\" id=\"img_id41540564212907\" width=\"1.0cm\" height=\"1.0cm\"><alt id=\"alt_id421540564212908\">Піктограма Скасувати упорядкування</alt></image>"

#: rep_insertfield.xhp
