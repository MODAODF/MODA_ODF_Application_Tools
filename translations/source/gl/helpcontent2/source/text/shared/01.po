"help.text"
msgid "<switchinline select=\"appl\"><caseinline select=\"WRITER\">Register-true</caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"WRITER\">Conformidade de rexistro</caseinline></switchinline>"

