msgctxt ""
"01190000.xhp\n"
"hd_id3147530\n"
"help.text"
msgid "Show"
msgstr "Hiện"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3153061\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/show\">Displays the entire comment for the selected version.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/show\">Hiển thị toàn ghi chú về phiên bản đã chọn.</ahelp>"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3154923\n"
"help.text"
msgid "Delete"
msgstr "Xoá"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3149669\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/delete\">Deletes the selected version.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/delete\">Xoá phiên bản đã chọn.</ahelp>"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3148739\n"
"help.text"
msgid "Compare"
msgstr "So sánh"

