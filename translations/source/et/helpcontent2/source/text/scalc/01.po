msgctxt ""
"05080200.xhp\n"
"par_id3148550\n"
"help.text"
msgid "<ahelp hid=\".uno:DeletePrintArea\">Removes the defined print area.</ahelp>"
msgstr "<ahelp hid=\".uno:DeletePrintArea\">Eemaldab valitud trükiala.</ahelp>"

#: 05080300.xhp
msgctxt ""
"05080300.xhp\n"
"tit\n"
"help.text"
msgid "Edit Print Ranges"
msgstr "Trükialade muutmine"

#: 05080300.xhp
