"par_id3149795\n"
"help.text"
msgid "<image id=\"img_id3155503\" src=\"cmd/sc_open.png\"><alt id=\"alt_id3155503\">Icon</alt></image>"
msgstr "<image id=\"img_id3155503\" src=\"cmd/sc_open.png\"><alt id=\"alt_id3155503\">Ikona</alt></image>"

