msgctxt ""
"05030300.xhp\n"
"par_id3149811\n"
"help.text"
msgid "<ahelp hid=\"HID_TAB_ENT_TYPE\">Select a field type.</ahelp>"
msgstr "<ahelp hid=\"HID_TAB_ENT_TYPE\">ជ្រើស​ប្រភេទ​វាល​មួយ ។</ahelp>"

