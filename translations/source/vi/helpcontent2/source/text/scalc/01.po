"12090102.xhp\n"
"hd_id3149207\n"
"help.text"
msgid "Total columns"
msgstr "Tổng số cột"

#: 12090102.xhp
msgctxt ""
"12090102.xhp\n"
"par_id3166426\n"
"help.text"
