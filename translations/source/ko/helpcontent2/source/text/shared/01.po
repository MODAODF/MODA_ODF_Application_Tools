"help.text"
msgid "<link href=\"text/shared/01/05040100.xhp\" name=\"Organizer\">Organizer</link>"
msgstr "<link href=\"text/shared/01/05040100.xhp\" name=\"관리\">관리</link>"

#: 05040100.xhp
msgctxt ""
