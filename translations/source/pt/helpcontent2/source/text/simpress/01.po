"par_id3155329\n"
"help.text"
msgid "<ahelp hid=\"modules/simpress/ui/dockinganimation/bitmap\">Combines images into a single image.</ahelp>"
msgstr "<ahelp hid=\"modules/simpress/ui/dockinganimation/bitmap\">Combina imagens numa única imagem.</ahelp>"

#: 06050000.xhp
