msgctxt ""
"04060106.xhp\n"
"par_id3155476\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_LOG10\">Returns the base-10 logarithm of a number.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_LOG10\">Retorna el logaritme de base-10 d'un nombre.</ahelp>"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3159294\n"
"help.text"
msgid "LOG10(Number)"
msgstr "LOG10(Nombre)"

