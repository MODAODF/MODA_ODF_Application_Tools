"04060106.xhp\n"
"par_id3145740\n"
"help.text"
msgid "<item type=\"input\">=CEILING(-11;-2;1)</item> returns -12"
msgstr "<item type=\"input\">=ARRODMULTSUP(-11;-2;1)</item> retorna -12."

