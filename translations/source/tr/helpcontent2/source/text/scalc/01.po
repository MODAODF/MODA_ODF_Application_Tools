"bm_id3159161\n"
"help.text"
msgid "<bookmark_value>ISOWEEKNUM function</bookmark_value>"
msgstr "<bookmark_value>HAFTASAY işlevi</bookmark_value>"

#: func_isoweeknum.xhp
msgctxt ""
