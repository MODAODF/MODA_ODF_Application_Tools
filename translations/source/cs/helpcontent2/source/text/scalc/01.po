msgctxt ""
"04060115.xhp\n"
"par_id3150662\n"
"help.text"
msgid "<item type=\"input\">=DEC2BIN(100;8)</item> returns 01100100."
msgstr "<item type=\"input\">=DEC2BIN(100;8)</item> vrátí 01100100."

