"par_id3155307\n"
"help.text"
msgid "Dim sText As Variant ' Must be a variant"
msgstr "Dim sText As Variant ' Måste vara av typen variant"

#: 03020201.xhp
msgctxt ""
"03020201.xhp\n"
"par_id3149411\n"
"help.text"
msgid "Seek #iNumber,1 ' Position at beginning"
msgstr "Seek #iNumber,1 ' Placera i början"

#: 03020201.xhp
msgctxt ""
"03020201.xhp\n"
"par_id3153158\n"
"help.text"
msgid "Put #iNumber,, \"This is the first line of text\" ' Fill line with text"
msgstr "Put #iNumber,, \"Det här är den första textraden\" ' Fyll raden med text"

#: 03020201.xhp
msgctxt ""
"03020201.xhp\n"
"par_id3148457\n"
"help.text"
msgid "Put #iNumber,, \"This is the second line of text\""
msgstr "Put #iNumber, \"Det här är den andra textraden\""

#: 03020201.xhp
