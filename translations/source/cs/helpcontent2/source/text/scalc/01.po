msgctxt ""
"04060182.xhp\n"
"par_id2954905\n"
"help.text"
msgid "<item type=\"input\">=HYPGEOM.DIST(2;2;90;100;1)</item> yields 1."
msgstr "<item type=\"input\">=HYPGEOM.DIST(2;2;90;100;1)</item> vrátí 1."

