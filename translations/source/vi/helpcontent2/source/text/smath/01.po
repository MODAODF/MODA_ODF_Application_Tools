"par_id3149126\n"
"help.text"
msgid "<ahelp hid=\"modules/smath/ui/fontdialog/italic\" visibility=\"visible\">Check this box to assign the italic attribute to the font.</ahelp>"
msgstr "<ahelp hid=\"modules/smath/ui/fontdialog/italic\" visibility=\"visible\">Chọn ô này để làm nghiêng chữ.</ahelp>"

#: 05020000.xhp
msgctxt ""
