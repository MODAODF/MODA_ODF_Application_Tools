"hd_id3149381\n"
"help.text"
msgid "Contents"
msgstr "Continguts"

#: 02110000.xhp
msgctxt ""
"02110000.xhp\n"
"par_id3150051\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/navigatorpanel/toggle\">Allows you to hide/show the contents.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/navigatorpanel/toggle\">Permet d'amagar o mostrar el contingut.</ahelp>"

