msgstr "Tükelda lahtrid"

#: 05100200.xhp
msgctxt ""
"05100200.xhp\n"
"par_id3083451\n"
"help.text"
msgid "<ahelp hid=\".\">Splits previously merged cells.</ahelp>"
msgstr "<ahelp hid=\".\">Tükeldab eelnevalt ühendatud lahtrid.</ahelp>"

#: 05100200.xhp
