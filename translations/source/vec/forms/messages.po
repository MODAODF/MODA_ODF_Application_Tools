msgid "To execute this function, parameters are needed."
msgstr "Par ezeguir 'sta funsion A ghe vol i paràmetri."

#: forms/inc/strings.hrc:50
msgctxt "RID_STR_FEATURE_NOT_EXECUTABLE"
msgid "This function cannot be executed, but is only for status queries."
msgstr "'Sta funsion no ła pol mìa èsar ezeguìa, ma ła ze vàłida soło par łe intarogasion so'l stato."

#: forms/inc/strings.hrc:51
msgctxt "RID_STR_FEATURE_UNKNOWN"
msgid "Unknown function."
msgstr "Funsion mìa conosùa."

#: forms/inc/strings.hrc:53
msgctxt "RID_STR_XFORMS_NO_BINDING_EXPRESSION"
msgid "Please enter a binding expression."
msgstr "Insarisi na espresion vincołante."

#: forms/inc/strings.hrc:54
msgctxt "RID_STR_XFORMS_INVALID_BINDING_EXPRESSION"
msgid "This is an invalid binding expression."
msgstr "'Sta espresion vincołante no ła ze mìa vàłida."

#: forms/inc/strings.hrc:55
msgctxt "RID_STR_XFORMS_INVALID_VALUE"
msgid "Value is invalid."
msgstr "Vałor mìa vàłido."

#: forms/inc/strings.hrc:56
msgctxt "RID_STR_XFORMS_REQUIRED"
msgid "A value is required."
msgstr "A ze nesesario un vałor."

#: forms/inc/strings.hrc:57
msgctxt "RID_STR_XFORMS_INVALID_CONSTRAINT"
msgid "The constraint '$1' not validated."
msgstr "El łìmite '$1' no'l ze mìa stà vałidà."

#: forms/inc/strings.hrc:58
msgctxt "RID_STR_XFORMS_VALUE_IS_NOT_A"
msgid "The value is not of the type '$2'."
msgstr "El vałor no'l ze mìa de tipo '$2'."

#: forms/inc/strings.hrc:59
msgctxt "RID_STR_XFORMS_VALUE_MAX_INCL"
msgid "The value must be smaller than or equal to $2."
msgstr "El vałor el ga da èsar pì picenin o guałivo a $2."

#: forms/inc/strings.hrc:60
msgctxt "RID_STR_XFORMS_VALUE_MAX_EXCL"
msgid "The value must be smaller than $2."
msgstr "El vałor el ga da èsar pì picenin de $2."

#: forms/inc/strings.hrc:61
msgctxt "RID_STR_XFORMS_VALUE_MIN_INCL"
msgid "The value must be greater than or equal to $2."
msgstr "El vałor el ga da èsar pì grando o guałivo a $2."

#: forms/inc/strings.hrc:62
msgctxt "RID_STR_XFORMS_VALUE_MIN_EXCL"
msgid "The value must be greater than $2."
msgstr "El vałor el ga da èsar pì grando de $2."

#: forms/inc/strings.hrc:63
msgctxt "RID_STR_XFORMS_VALUE_TOTAL_DIGITS"
msgid "$2 digits allowed at most."
msgstr "El nùmaro màsimo de sifre parmese el ze $2."

#: forms/inc/strings.hrc:64
msgctxt "RID_STR_XFORMS_VALUE_FRACTION_DIGITS"
msgid "$2 fraction digits allowed at most."
msgstr "El nùmaro màsimo de sifre desimałi parmese el ze $2."

#: forms/inc/strings.hrc:65
msgctxt "RID_STR_XFORMS_VALUE_LENGTH"
msgid "The string must be $2 characters long."
msgstr "Ła łonghesa de ła stringa ła ga da èsar de $2 caràtari."

#: forms/inc/strings.hrc:66
msgctxt "RID_STR_XFORMS_VALUE_MIN_LENGTH"
msgid "The string must be at least $2 characters long."
msgstr "Ła łonghesa mìnima de ła stringa ła ga da èsar de $2 caràtari."

#: forms/inc/strings.hrc:67
msgctxt "RID_STR_XFORMS_VALUE_MAX_LENGTH"
msgid "The string can only be $2 characters long at most."
msgstr "Ła łonghesa màsima de ła stringa ła pol èsar de $2 caràtari."

