"03090301.xhp\n"
"par_id3150329\n"
"help.text"
msgid "iInputb = Int(InputBox(\"Enter the second number: \",\"NumberInput\"))"
msgstr "iInputb = Int(InputBox(\"Ange det andra talet: \",\"Nummerinmatning\"))"

