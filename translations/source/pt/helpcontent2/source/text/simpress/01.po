msgid "<ahelp hid=\"modules/simpress/ui/dockinganimation/alignment\">Aligns the images in your animation.</ahelp>"
msgstr "<ahelp hid=\"modules/simpress/ui/dockinganimation/alignment\">Alinha as imagens da sua animação.</ahelp>"

#: 06050000.xhp
