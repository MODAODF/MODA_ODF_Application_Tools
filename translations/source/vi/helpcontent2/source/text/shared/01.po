msgstr "<ahelp hid=\".\">Xác định hạng của thuật ngữ đã chọn.</ahelp>"

#: 06010601.xhp
msgctxt ""
"06010601.xhp\n"
"par_idN10593\n"
"help.text"
msgid "Add"
msgstr "Thêm"

