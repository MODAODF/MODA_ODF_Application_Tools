msgctxt ""
"04060107.xhp\n"
"par_id3144871\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_SUMMEX2MY2\">Returns the sum of the difference of squares of corresponding values in two arrays.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_SUMMEX2MY2\">İki dizininin karşılık gelen elemanlarının karelerinin farkını alarak toplamı bulur.</ahelp>"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"par_id3144903\n"
"help.text"
msgid "SUMX2MY2(ArrayX; ArrayY)"
msgstr "TOPX2EY2(dizi_x; dizi_y)"

