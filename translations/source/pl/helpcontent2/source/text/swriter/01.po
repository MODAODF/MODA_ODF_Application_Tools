"01150000.xhp\n"
"par_id3155910\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/mailmerge/file\">Saves the form letters in files.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/mailmerge/file\">Zapisuje listy seryjne w plikach.</ahelp>"

