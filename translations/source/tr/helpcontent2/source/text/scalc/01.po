msgctxt ""
"func_networkdays.xhp\n"
"par_id241030160012187036\n"
"help.text"
msgid "<link href=\"text/scalc/01/func_workday.xhp\">WORKDAYS</link>"
msgstr "<link href=\"text/scalc/01/func_workday.xhp\">TAMİŞGÜNÜ</link>"

