msgctxt ""
"05220000.xhp\n"
"par_idN10724\n"
"help.text"
msgid "<ahelp hid=\".\">Resizes a custom shape to fit the text that you enter after double-clicking the shape.</ahelp>"
msgstr "<ahelp hid=\".\">Změní velikost tvaru, aby odpovídal zapsanému textu.</ahelp>"

#: 05220000.xhp
msgctxt ""
