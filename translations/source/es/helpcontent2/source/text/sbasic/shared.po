msgctxt ""
"03060600.xhp\n"
"hd_id3153968\n"
"help.text"
msgid "Parameters:"
msgstr "Parámetros:"

#: 03060600.xhp
msgctxt ""
"03060600.xhp\n"
"par_id3150448\n"
"help.text"
msgid "<emph>Result:</emph> Any numeric variable that contains the result of the combination."
msgstr "<emph>Resultado:</emph> cualquier variable numérica que contenga el resultado de la combinación."

