msgstr "UNICHAR"

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id0907200904022538\n"
"help.text"
msgid "<ahelp hid=\".\">Converts a code number into a Unicode character or letter.</ahelp>"
msgstr "<ahelp hid=\".\">Převede číselný kód na znak nebo písmeno Unicode.</ahelp>"

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id0907200904123753\n"
"help.text"
msgid "<item type=\"literal\">UNICHAR(number)</item>"
msgstr "<item type=\"literal\">UNICHAR(číslo)</item>"

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id090720090412378\n"
"help.text"
msgid "=UNICHAR(169) returns the Copyright character <emph>©</emph>."
msgstr "=UNICHAR(169) vrátí znak copyright - <emph>©</emph>."

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id050220170755399756\n"
