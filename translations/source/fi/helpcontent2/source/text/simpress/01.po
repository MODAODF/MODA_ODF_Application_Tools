msgctxt ""
"04080100.xhp\n"
"par_id3146975\n"
"help.text"
msgid "<variable id=\"tabelletext\"><ahelp hid=\".\">Inserts a new table into the current slide or page.</ahelp></variable>"
msgstr "<variable id=\"tabelletext\"><ahelp hid=\".\">Lisätään uusi taulukko käsiteltävälle dialle tai sivulle.</ahelp></variable>"

#: 04110000.xhp
msgctxt ""
"04110000.xhp\n"
