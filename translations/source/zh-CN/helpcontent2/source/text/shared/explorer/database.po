msgctxt ""
"querywizard02.xhp\n"
"par_idN105B2\n"
"help.text"
msgid "<ahelp hid=\".\">Specifies the field by which the created query is sorted.</ahelp>"
msgstr "<ahelp hid=\".\">指定已创建查询排序所依据的字段。</ahelp>"

