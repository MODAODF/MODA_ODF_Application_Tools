msgctxt ""
"mailmerge00.xhp\n"
"par_idN105CC\n"
"help.text"
msgid "First step: <link href=\"text/swriter/01/mailmerge01.xhp\" name=\"Mail Merge Wizard - Select starting document\">Mail Merge Wizard - Select starting document</link>."
msgstr "Première étape : <link href=\"text/swriter/01/mailmerge01.xhp\" name=\"Mail Merge Wizard - Select starting document\">Assistant Mailing - Sélectionner le document de base</link>."

#: mailmerge00.xhp
msgctxt ""
