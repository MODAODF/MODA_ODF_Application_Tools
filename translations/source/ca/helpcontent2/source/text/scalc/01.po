msgctxt ""
"04060116.xhp\n"
"par_id2480849\n"
"help.text"
msgid "For even numbers FACTDOUBLE(n) returns:"
msgstr "Per als nombres parells, DOBLEFACT(n) retorna:"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id4181951\n"
"help.text"
msgid "2*4*6*8* ... *n"
msgstr "2*4*6*8* ... *n"

