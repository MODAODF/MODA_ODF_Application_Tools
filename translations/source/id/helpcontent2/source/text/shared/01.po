msgid "<link href=\"text/shared/01/03050000.xhp\" name=\"Tools Bar\">Tools Bar</link>"
msgstr "<link href=\"text/shared/01/03050000.xhp\" name=\"Tools Bar\">Bilah Alat</link>"

#: 03050000.xhp
