msgctxt ""
"dabawiz02jdbc.xhp\n"
"par_idN106E7\n"
"help.text"
msgid "<link href=\"text/shared/explorer/database/dabawiz03auth.xhp\">Authentication</link>"
msgstr "<link href=\"text/shared/explorer/database/dabawiz03auth.xhp\">验证</link>"

