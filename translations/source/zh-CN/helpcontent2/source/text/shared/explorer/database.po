msgstr "高级属性"

#: menuedit.xhp
msgctxt ""
"menuedit.xhp\n"
"par_idN105F7\n"
"help.text"
msgid "<ahelp hid=\".\">Opens the Advanced Properties dialog.</ahelp>"
msgstr "<ahelp hid=\".\">打开「高级属性」对话框。</ahelp>"

