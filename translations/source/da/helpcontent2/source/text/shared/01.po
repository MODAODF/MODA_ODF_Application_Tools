msgid "Title"
msgstr "Titel"

#: 01100100.xhp
msgctxt ""
"01100100.xhp\n"
"par_id3156042\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/descriptioninfopage/title\">Enter a title for the document.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/descriptioninfopage/title\">Indtast en titel på dokumentet.</ahelp>"

