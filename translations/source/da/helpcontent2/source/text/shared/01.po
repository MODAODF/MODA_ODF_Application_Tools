msgstr "<image id=\"img_id083120160608495768\" src=\"media/screenshots/cui/ui/charnamepage/CharNamePage.png\" width=\"9.0417in\" height=\"6.9791in\"><alt id=\"alt_id083120160608495768\">Skrifttypedialog</alt></image>"

#: 05020100.xhp
