#: forms/inc/strings.hrc:71
msgctxt "RID_STR_DATATYPE_DECIMAL"
msgid "Decimal"
msgstr "ទសភាគ"

#: forms/inc/strings.hrc:72
msgctxt "RID_STR_DATATYPE_FLOAT"
msgid "Floating point"
