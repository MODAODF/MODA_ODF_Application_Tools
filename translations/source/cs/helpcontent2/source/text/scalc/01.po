"par_id3150751\n"
"help.text"
msgid "<image id=\"img_id3153714\" src=\"sc/res/sf01.png\"><alt id=\"alt_id3153714\">Icon</alt></image>"
msgstr "<image id=\"img_id3153714\" src=\"sc/res/sf01.png\"><alt id=\"alt_id3153714\">Ikona</alt></image>"

