msgctxt ""
"03080104.xhp\n"
"par_id3154366\n"
"help.text"
msgid "Tan(Alpha) = side opposite the angle/side adjacent to angle"
msgstr "Tan(Alfa) = lado oposto ao ângulo/lado adjacente ao ângulo"

