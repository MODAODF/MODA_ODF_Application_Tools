msgctxt ""
"01170101.xhp\n"
"par_id3150690\n"
"help.text"
msgid "Yes"
msgstr "Jah"

#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"par_id3159106\n"
"help.text"
msgid "The control can be selected with the Tab key."
msgstr "Juhtelementi saab valida Tab-klahviga."

#: 01170101.xhp
msgctxt ""
