"par_id3154280\n"
"help.text"
msgid "<image id=\"img_id3154923\" src=\"cmd/sc_saveas.png\"><alt id=\"alt_id3154923\">Icon</alt></image>"
msgstr "<image id=\"img_id3154923\" src=\"cmd/sc_saveas.png\"><alt id=\"alt_id3154923\">Ikona</alt></image>"

