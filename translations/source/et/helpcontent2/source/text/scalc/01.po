"04050000.xhp\n"
"par_id3150718\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/insertsheet/nameed\">Specifies the name of the new sheet.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/insertsheet/nameed\">Määrab uue lehe nime.</ahelp>"

