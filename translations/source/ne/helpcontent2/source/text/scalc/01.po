msgstr "<ahelp hid=\"HID_FUNC_PHI\">मानक सामान्य वितरणका लागि वितरण प्रकार्यको मानहरू फर्काउँदछ ।</ahelp>"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3149976\n"
"help.text"
msgid "PHI(Number)"
msgstr "PHI(सङ्ख्या)"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3156108\n"
"help.text"
msgid "<emph>Number</emph> represents the value based on which the standard normal distribution is calculated."
msgstr "<emph>सङ्ख्या</emph> आधारित मान हो जसलाई मानक सामान्य वितरणमा गणना गरिन्छ ।"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id3155849\n"
"help.text"
msgid "<item type=\"input\">=PHI(2.25) </item>= 0.03"
msgstr "<item type=\"input\">=PHI(2.25) </item>= ०.०३"

