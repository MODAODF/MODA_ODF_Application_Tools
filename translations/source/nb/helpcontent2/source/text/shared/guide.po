msgctxt ""
"xsltfilter.xhp\n"
"par_id6426892\n"
"help.text"
msgid "<link href=\"text/shared/guide/xsltfilter_create.xhp\">Creating and Testing XML filters</link>"
msgstr "<link href=\"text/shared/guide/xsltfilter_create.xhp\">Lage og teste XML-filtere</link>"

