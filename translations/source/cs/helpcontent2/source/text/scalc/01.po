msgctxt ""
"05100000.xhp\n"
"hd_id3153963\n"
"help.text"
msgid "Page Styles"
msgstr "Styly stránky"

#: 05100000.xhp
msgctxt ""
"05100000.xhp\n"
"par_id3147003\n"
"help.text"
msgid "<ahelp hid=\".\">Displays the Page Styles available for indirect page formatting.</ahelp>"
msgstr "<ahelp hid=\".\">Zobrazí seznam stylů stránky dostupných pro nepřímé formátování stránek.</ahelp>"

