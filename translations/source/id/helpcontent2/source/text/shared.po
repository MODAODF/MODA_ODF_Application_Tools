msgctxt ""
"main0201.xhp\n"
"tit\n"
"help.text"
msgid "Standard Bar"
msgstr "Baris Standar"

#: main0201.xhp
msgctxt ""
"main0201.xhp\n"
"hd_id3154186\n"
"help.text"
msgid "<link href=\"text/shared/main0201.xhp\" name=\"Standard Bar\">Standard Bar</link>"
msgstr "<link href=\"text/shared/main0201.xhp\" name=\"Standard Bar\">Baris Standar</link>"

#: main0201.xhp
msgctxt ""
"main0201.xhp\n"
"par_id3145136\n"
