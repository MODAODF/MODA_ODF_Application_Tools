msgstr "インストールは失敗しました。"

#: Error.ulf
msgctxt ""
"Error.ulf\n"
"OOO_ERROR_84\n"
"LngText.text"
msgid "Product: [2] -- [3]"
msgstr "製品:[2] -- [3]"

#: Error.ulf
msgctxt ""
"Error.ulf\n"
"OOO_ERROR_85\n"
"LngText.text"
