msgctxt ""
"04060107.xhp\n"
"par_id3166091\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_TREND\">Returns values along a linear trend.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_TREND\">Trả về các giá trị dọc theo một xu hướng tuyến tính.</ahelp>"

