msgctxt ""
"insert_footnote_endnote.xhp\n"
"par_id030420161138373075\n"
"help.text"
msgid "<ahelp hid=\".uno:InsertFootnote\">Insert a footnote at the current cursor position without a prompt.</ahelp>"
