msgid "<ahelp hid=\".\" visibility=\"hidden\">Select this icon to browse through index entries.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Selecteer dit pictogram om door indexitems te bladeren.</ahelp>"

#: 02110100.xhp
