#: 06030000.xhp
msgctxt ""
"06030000.xhp\n"
"hd_id3149495\n"
"help.text"
msgid "Hyphenate"
msgstr "སྦྲེལ་ཡིག་རྟགས།"

#: 06030000.xhp
msgctxt ""
"06030000.xhp\n"
"par_id3149096\n"
