msgctxt ""
"fields_enter.xhp\n"
"par_id3155620\n"
"help.text"
msgid "Click “Input field”in the <item type=\"menuitem\">Type</item> list."
msgstr "V seznamu <item type=\"menuitem\">Typ</item> klikněte na \"Vstupní pole\"."

