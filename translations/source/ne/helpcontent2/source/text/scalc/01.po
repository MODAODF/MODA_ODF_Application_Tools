msgctxt ""
"04060119.xhp\n"
"par_id3150224\n"
"help.text"
msgid "TBILLEQ(Settlement; Maturity; Discount)"
msgstr "TBILLEQ(बन्दोबस्त;परिपक्व;छुट)"

