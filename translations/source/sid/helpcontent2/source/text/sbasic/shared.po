msgctxt ""
"03090303.xhp\n"
"par_id3153948\n"
"help.text"
msgid "sVar =sVar & \" From Sub 1 to\" : Return"
msgstr "sVar =sVar & \" Cinaancho 1 nni...wido\" : Qolo"

#: 03090303.xhp
msgctxt ""
"03090303.xhp\n"
"par_id3153708\n"
"help.text"
msgid "sVar =sVar & \" From Sub 2 to\" : Return"
msgstr "sVar =sVar & \" Cinaancho 2 nni...wido\" : Qolo"

