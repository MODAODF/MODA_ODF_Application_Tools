msgid "<image id=\"img_id3155503\" src=\"cmd/sc_open.png\"><alt id=\"alt_id3155503\">Icon</alt></image>"
msgstr "<image id=\"img_id3155503\" src=\"cmd/sc_open.png\"><alt id=\"alt_id3155503\">Ikon</alt></image>"

#: 02220000.xhp
