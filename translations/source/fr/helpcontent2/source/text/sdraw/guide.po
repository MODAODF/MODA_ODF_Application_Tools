msgctxt ""
"main.xhp\n"
"hd_id3143218\n"
"help.text"
msgid "Editing and Grouping Objects"
msgstr "Édition et groupement d'objets"

#: main.xhp
msgctxt ""
"main.xhp\n"
"hd_id3149018\n"
"help.text"
msgid "Editing Colors and Textures"
msgstr "Édition des couleurs et des textures"

#: main.xhp
