msgstr "Pagina intera"

#: 10020000.xhp
msgctxt ""
"10020000.xhp\n"
"par_id3153582\n"
"help.text"
msgid "<ahelp hid=\".uno:ZoomPage\">Displays the entire slide on your screen.</ahelp>"
msgstr "<ahelp hid=\".uno:ZoomPage\">Mostra l'intera diapositiva sullo schermo.</ahelp>"

