"help.text"
msgid "<link href=\"text/sbasic/shared/01020000.xhp\" name=\"Syntax\">Syntax</link>"
msgstr "<link href=\"text/sbasic/shared/01020000.xhp\" name=\"Syntax\">Ganallo</link>"

#: 01020000.xhp
msgctxt ""
