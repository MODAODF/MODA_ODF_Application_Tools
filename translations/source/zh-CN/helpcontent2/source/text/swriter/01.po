msgctxt ""
"mailmerge01.xhp\n"
"par_idN10561\n"
"help.text"
msgid "<ahelp hid=\".\">Creates a new Writer document to use for the mail merge.</ahelp>"
msgstr "<ahelp hid=\".\">创建新的 Writer 文档以用于邮件合并。</ahelp>"

