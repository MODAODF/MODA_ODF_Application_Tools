"bm_id6009094\n"
"help.text"
msgid "<bookmark_value>wizards;database tables (Base)</bookmark_value><bookmark_value>Table Wizard (Base)</bookmark_value>"
msgstr "<bookmark_value>마법사;데이터베이스 테이블(Base)</bookmark_value><bookmark_value>테이블 마법사(Base)</bookmark_value>"

#: tablewizard00.xhp
msgctxt ""
"tablewizard00.xhp\n"
"par_idN1054C\n"
"help.text"
