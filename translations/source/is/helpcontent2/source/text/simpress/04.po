msgctxt ""
"01020000.xhp\n"
"hd_id3149543\n"
"help.text"
msgid "Shift+drag (when resizing)"
msgstr "Shift+draga (þegar verið er að stilla stærð)"

#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"par_id3150689\n"
"help.text"
msgid "Hold down Shift while dragging to resize an object to maintain the proportions of the object."
