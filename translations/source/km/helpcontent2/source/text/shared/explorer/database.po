"par_idN1054D\n"
"help.text"
msgid "<link href=\"text/shared/explorer/database/menuedit.xhp\">Edit</link>"
msgstr "<link href=\"text/shared/explorer/database/menuedit.xhp\">កែសម្រួល</link>"

#: menuedit.xhp
