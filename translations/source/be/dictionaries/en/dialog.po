msgctxt ""
"en_en_US.properties\n"
"apostrophe\n"
"property.text"
msgid "Apostrophe"
msgstr "Апостраф"

#: en_en_US.properties
msgctxt ""
"en_en_US.properties\n"
"hlp_ellipsis\n"
"property.text"
