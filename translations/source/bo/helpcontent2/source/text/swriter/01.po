msgctxt ""
"05030200.xhp\n"
"par_id3154574\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/textflowpage/checkInsert\">Select this check box, and then select the break type that you want to use.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/textflowpage/checkInsert\">བསྐྱར་འདེམས་སྒྲོམ་བདམས་རྗེས་བེད་སྤྱོད་དགོས་པའི་ཤོག་དབྱེའི་རྟགས་ཀྱི་རིགས་བདམས་དགོས།</ahelp>"

