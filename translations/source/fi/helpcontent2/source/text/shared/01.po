msgstr "Yksi sivu"

#: ref_pdf_export.xhp
msgctxt ""
"ref_pdf_export.xhp\n"
"par_id672322\n"
"help.text"
msgid "<ahelp hid=\".\">Select to generate a PDF file that shows one page at a time.</ahelp>"
msgstr "<ahelp hid=\".\">Valitaan tuotettavaksi PDF-tiedosto, joka esittää sivun kerrallaan.</ahelp>"

#: ref_pdf_export.xhp
msgctxt ""
