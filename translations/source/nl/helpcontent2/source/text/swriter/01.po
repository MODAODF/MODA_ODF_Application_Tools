msgid "<image id=\"img_id3159200\" src=\"sw/res/sf03.png\"><alt id=\"alt_id3159200\">Icon</alt></image>"
msgstr "<image id=\"img_id3159200\" src=\"sw/res/sf03.png\"><alt id=\"alt_id3159200\">Pictogram</alt></image>"

#: 05140000.xhp
msgctxt ""
"05140000.xhp\n"
"par_id3151332\n"
"help.text"
msgid "Frame Styles"
msgstr "Frameopmaakprofielen"

#: 05140000.xhp
msgctxt ""
"05140000.xhp\n"
"par_id3143282\n"
"help.text"
msgid "<ahelp hid=\".\">Displays formatting styles for frames.</ahelp> Use frame styles to format frame layouts and position."
msgstr "<ahelp hid=\".\">Toont opmaakprofielen voor frames.</ahelp> Gebruik frame-opmaakprofielen voor de opmaak van frame-layouts en -positie."

#: 05140000.xhp
msgctxt ""
"05140000.xhp\n"
"par_id3149819\n"
"help.text"
msgid "<image id=\"img_id3149826\" src=\"sw/res/sf04.png\"><alt id=\"alt_id3149826\">Icon</alt></image>"
msgstr "<image id=\"img_id3149826\" src=\"sw/res/sf04.png\"><alt id=\"alt_id3149826\">Pictogram</alt></image>"

