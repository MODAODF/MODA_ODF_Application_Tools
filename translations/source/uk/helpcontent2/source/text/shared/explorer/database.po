msgstr "Умова"

#: querywizard03.xhp
msgctxt ""
"querywizard03.xhp\n"
"par_idN105D9\n"
"help.text"
msgid "<ahelp hid=\".\">Select the condition for the filter.</ahelp>"
msgstr "<ahelp hid=\".\">Оберіть умову для фільтра.</ahelp>"

#: querywizard03.xhp
msgctxt ""
"querywizard03.xhp\n"
"par_idN1056E\n"
"help.text"
msgid "Value"
msgstr "Значення"

#: querywizard03.xhp
msgctxt ""
"querywizard03.xhp\n"
"par_idN105E6\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the value for the filter condition.</ahelp>"
msgstr "<ahelp hid=\".\">Введіть значення для умови фільтра.</ahelp>"

