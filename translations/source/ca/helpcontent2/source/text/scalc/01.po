msgctxt ""
"04060185.xhp\n"
"par_id3149289\n"
"help.text"
msgid "TINV(Number; DegreesFreedom)"
msgstr "INVT(Nombre; GrausDeLlibertat)"

#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
"par_id3154070\n"
"help.text"
