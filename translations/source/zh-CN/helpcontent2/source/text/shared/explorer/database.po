msgctxt ""
"querywizard04.xhp\n"
"par_idN105BD\n"
"help.text"
msgid "<ahelp hid=\".\">Select to show all records of the query.</ahelp>"
msgstr "<ahelp hid=\".\">选择显示所有查询记录。</ahelp>"

