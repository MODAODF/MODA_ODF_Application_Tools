#: 05090201.xhp
msgctxt ""
"05090201.xhp\n"
"bm_id3154506\n"
"help.text"
msgid "<bookmark_value>tables; editing with the keyboard</bookmark_value>"
msgstr "<bookmark_value>རིའུ་མིག་མཐེབ་གཞོང་སྤྱད་དེ་རྩོམ་སྒྲིག་</bookmark_value>"

