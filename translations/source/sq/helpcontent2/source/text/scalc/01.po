"help.text"
msgid "CONCATENATE"
msgstr "CONCATENATE"

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id3154524\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_VERKETTEN\">Combines several text strings into one string.</ahelp>"
msgstr ""

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id3155954\n"
"help.text"
msgid "CONCATENATE(\"Text1\"; ...; \"Text30\")"
msgstr ""

