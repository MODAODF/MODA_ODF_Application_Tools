msgctxt ""
"03090410.xhp\n"
"par_id3149579\n"
"help.text"
msgid "sGender = GetGenderIndex( \"John\" )"
msgstr "sGender = GetGenderIndex( \"João\" )"

#: 03090410.xhp
msgctxt ""
"03090410.xhp\n"
"par_id3153361\n"
"help.text"
msgid "GetGenderIndex = Switch(sName = \"Jane\", \"female\", sName = \"John\", \"male\")"
msgstr "GetGenderIndex = Switch(sName = \"Joana\", \"feminino\", sName = \"João\", \"masculino\")"

