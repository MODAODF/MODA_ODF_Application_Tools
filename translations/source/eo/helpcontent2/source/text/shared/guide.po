msgctxt ""
"startcenter.xhp\n"
"par_id082020080310499\n"
"help.text"
msgid "Create:"
msgstr ""

#: startcenter.xhp
msgctxt ""
"startcenter.xhp\n"
"par_id0820200803104810\n"
"help.text"
msgid "<ahelp hid=\".\">The document buttons each open a new document of the specified type.</ahelp>"
msgstr ""

