msgctxt ""
"06040100.xhp\n"
"par_id3153127\n"
"help.text"
msgid "This feature does not work if the formatting characters <item type=\"literal\">* / - _</item> are entered with an <link href=\"text/shared/00/00000005.xhp#IME\" name=\"Input Method Editor\">Input Method Editor</link>."
msgstr ""

