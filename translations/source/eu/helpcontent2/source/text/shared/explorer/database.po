msgstr "Hautatutako eremuak"

#: tablewizard02.xhp
msgctxt ""
"tablewizard02.xhp\n"
"par_idN10569\n"
"help.text"
msgid "<ahelp hid=\".\">Select a field in order to edit the field information.</ahelp>"
msgstr "<ahelp hid=\".\">Hautatu eremu bat eremu-informazioa editatzeko.</ahelp>"

