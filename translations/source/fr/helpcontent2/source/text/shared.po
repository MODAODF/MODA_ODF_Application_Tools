"3dsettings_toolbar.xhp\n"
"par_idN105CA\n"
"help.text"
msgid "<ahelp hid=\".\">Tilts the selected object left by five degrees.</ahelp>"
msgstr "<ahelp hid=\".\">Incline l'objet sélectionné de 5 degrés vers la gauche.</ahelp>"

