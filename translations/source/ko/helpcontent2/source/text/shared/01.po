"hd_id3152474\n"
"help.text"
msgid "<link href=\"text/shared/01/05290200.xhp\" name=\"Remove\">Ungroup</link>"
msgstr "<link href=\"text/shared/01/05290200.xhp\" name=\"제거\">그룹 해제</link>"

#: 05290000.xhp
msgctxt ""
"05290000.xhp\n"
"hd_id3145609\n"
"help.text"
