msgctxt ""
"04060184.xhp\n"
"par_id3149283\n"
"help.text"
msgid "<item type=\"input\">=NORMDIST(70;63;5;0)</item> returns 0.03."
msgstr "<item type=\"input\">=NORMDIST(70;63;5;0)</item> vrátí 0,03."

