msgctxt ""
"06050000.xhp\n"
"par_id3157974\n"
"help.text"
msgid "<ahelp hid=\"modules/simpress/ui/dockinganimation/getone\">Adds selected object(s) as a single image.</ahelp>"
msgstr "<ahelp hid=\"modules/simpress/ui/dockinganimation/getone\">Lisätään valitut objektit yhtenä kuvana.</ahelp>"

#: 06050000.xhp
msgctxt ""
"06050000.xhp\n"
"par_id3156068\n"
"help.text"
msgid "<image id=\"img_id3148768\" src=\"sd/res/get1obj.png\" width=\"0.1665in\" height=\"0.1665in\"><alt id=\"alt_id3148768\">Icon</alt></image>"
msgstr "<image id=\"img_id3148768\" src=\"sd/res/get1obj.png\" width=\"0.1665in\" height=\"0.1665in\"><alt id=\"alt_id3148768\">Liitä objekti -kuvake, jossa suorakaide ja osoitin</alt></image>"

#: 06050000.xhp
msgctxt ""
"06050000.xhp\n"
"par_id3150268\n"
"help.text"
msgid "Apply Object"
msgstr "Liitä objekti"

