msgctxt ""
"03080102.xhp\n"
"par_id3154491\n"
"help.text"
msgid "dAngle = InputBox(\"Enter the angle Alpha (in degrees): \",\"Alpha\")"
msgstr "dAngle = InputBox(\"Introduza o ângulo Alfa (em graus): \",\"Alfa\")"

