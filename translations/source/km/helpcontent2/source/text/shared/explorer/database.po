"hd_id3149416\n"
"help.text"
msgid "<<"
msgstr "<<"

#: 11030100.xhp
msgctxt ""
"11030100.xhp\n"
"par_id3145315\n"
"help.text"
msgid "<ahelp hid=\"dbaccess/ui/dbaseindexdialog/addall\">Moves all of the free indexes to the <emph>Table Indexes</emph> list.</ahelp>"
msgstr "<ahelp hid=\"dbaccess/ui/dbaseindexdialog/addall\">ផ្លាស់ទី​សន្ទស្សន៍​ទំនេរ​ទាំងអស់​ទៅ​បញ្ជី <emph>សន្ទស្សន៍​តារាង</emph> ។</ahelp>"

