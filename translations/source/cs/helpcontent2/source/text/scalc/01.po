msgstr "1*3*5*7* ... *n"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id4071779\n"
"help.text"
msgid "FACTDOUBLE(0) returns 1 by definition."
msgstr "FACTDOUBLE(0) vrací 1 (z definice)."

