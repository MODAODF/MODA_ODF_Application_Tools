msgctxt ""
"tablewizard04.xhp\n"
"par_idN1055A\n"
"help.text"
msgid "<ahelp hid=\".\">Specifies the table name.</ahelp>"
msgstr "<ahelp hid=\".\">Taularen izena zehazten du.</ahelp>"

#: tablewizard04.xhp
msgctxt ""
"tablewizard04.xhp\n"
"par_idN105E4\n"
"help.text"
msgid "Catalog of the table"
msgstr "Taularen katalogoa"

#: tablewizard04.xhp
msgctxt ""
"tablewizard04.xhp\n"
"par_idN105EA\n"
"help.text"
msgid "<ahelp hid=\".\">Select the catalog for the table. (Available only if the database supports catalogs)</ahelp>"
msgstr "<ahelp hid=\".\">Taularen katalogoa hautatzen du. (Aukera hori erabilgarri egongo da datu-baseak katalogoak onartzen baditu)</ahelp>"

#: tablewizard04.xhp
msgctxt ""
"tablewizard04.xhp\n"
"par_idN10605\n"
"help.text"
msgid "Schema of the table"
msgstr "Taularen eskema"

#: tablewizard04.xhp
msgctxt ""
"tablewizard04.xhp\n"
"par_idN1060B\n"
"help.text"
msgid "<ahelp hid=\".\">Select the schema for the table. (Available only if the database supports schemas)</ahelp>"
msgstr "<ahelp hid=\".\">Hautatu taularen eskema. (Aukera hori erabilgarri egongo da datu-baseak eskemak onartzen baditu)</ahelp>"

#: tablewizard04.xhp
msgctxt ""
"tablewizard04.xhp\n"
"par_idN1055D\n"
"help.text"
msgid "Modify the table design"
msgstr "Aldatu taularen diseinua"

#: tablewizard04.xhp
msgctxt ""
"tablewizard04.xhp\n"
"par_idN10561\n"
