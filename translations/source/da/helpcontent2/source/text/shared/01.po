#: xformsdatatab.xhp
msgctxt ""
"xformsdatatab.xhp\n"
"hd_id7594225\n"
"help.text"
msgid "Min. (inclusive)"
msgstr "Min. (inklusiv)"

#: xformsdatatab.xhp
msgctxt ""
"xformsdatatab.xhp\n"
"par_id8147221\n"
"help.text"
msgid "<ahelp hid=\".\">Specifies an inclusive lower bound for values.</ahelp>"
msgstr "<ahelp hid=\".\">Specificerer en inklusiv nedre grænse for værdier.</ahelp>"

#: xformsdatatab.xhp
msgctxt ""
"xformsdatatab.xhp\n"
"hd_id5081637\n"
"help.text"
msgid "Min. (exclusive)"
msgstr "Min. (eksklusiv)"

#: xformsdatatab.xhp
msgctxt ""
"xformsdatatab.xhp\n"
