msgctxt ""
"03030206.xhp\n"
"par_id3149378\n"
"help.text"
msgid "a1 = \"start time\""
msgstr "a1 = \"सुरु समय\""

#: 03030206.xhp
msgctxt ""
"03030206.xhp\n"
"par_id3145800\n"
"help.text"
msgid "b1 = \"end time\""
msgstr "b1 = \"अन्त्य समय\""

#: 03030206.xhp
