msgctxt ""
"01020000.xhp\n"
"par_id3149417\n"
"help.text"
msgid "<link href=\"text/shared/guide/doc_open.xhp\" name=\"Opening Documents\">Opening Documents</link>"
msgstr "<link href=\"text/shared/guide/doc_open.xhp\" name=\"Opening Documents\">Membuka Dokumen</link>"

