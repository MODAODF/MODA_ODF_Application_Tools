msgctxt ""
"03120306.xhp\n"
"par_id3153189\n"
"help.text"
msgid "sInput = InputBox(\"Please input a date in the international format 'YYYY-MM-DD'\")"
msgstr "sEntrada = InputBox(\"Escriba una fecha en formato internacional 'AAAA-MM-DD'\")"

