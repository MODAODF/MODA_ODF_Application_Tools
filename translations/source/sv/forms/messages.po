#: forms/inc/strings.hrc:25
msgctxt "RID_BASELISTBOX_ERROR_FILLLIST"
msgid "The contents of a combo box or list field could not be determined."
msgstr "Innehållet i ett kombinationsfält eller en listruta kunde inte bestämmas."

#: forms/inc/strings.hrc:26
msgctxt "RID_STR_IMPORT_GRAPHIC"
msgid "Insert Image"
msgstr "Infoga bild"

#: forms/inc/strings.hrc:27
msgctxt "RID_STR_CONTROL_SUBSTITUTED_NAME"
msgid "substituted"
msgstr "ersatt"

#: forms/inc/strings.hrc:28
msgctxt "RID_STR_CONTROL_SUBSTITUTED_EPXPLAIN"
msgid "An error occurred while this control was being loaded. It was therefore replaced with a placeholder."
msgstr "När denna control laddades uppstod ett fel. Därför ersattes den av en platshållare."

#: forms/inc/strings.hrc:29
msgctxt "RID_STR_READERROR"
msgid "Error reading data from database"
msgstr "Fel vid läsning av data från databasen"

#: forms/inc/strings.hrc:30
msgctxt "RID_STR_CONNECTERROR"
msgid "Connection failed"
msgstr "Uppkoppling avbruten"

#: forms/inc/strings.hrc:31
msgctxt "RID_ERR_LOADING_FORM"
msgid "The data content could not be loaded."
msgstr "Datainnehållet kunde inte laddas."

#: forms/inc/strings.hrc:32
msgctxt "RID_ERR_REFRESHING_FORM"
msgid "The data content could not be updated"
msgstr "Datainnehållet kunde inte uppdateras."

#: forms/inc/strings.hrc:33
msgctxt "RID_STR_ERR_INSERTRECORD"
msgid "Error inserting the new record"
msgstr "Fel vid infogning av den nya dataposten"

#: forms/inc/strings.hrc:34
msgctxt "RID_STR_ERR_UPDATERECORD"
msgid "Error updating the current record"
msgstr "Fel vid skrivning av den aktuella dataposten"

#: forms/inc/strings.hrc:35
msgctxt "RID_STR_ERR_DELETERECORD"
msgid "Error deleting the current record"
msgstr "Fel vid radering av den aktuella dataposten"

#: forms/inc/strings.hrc:36
msgctxt "RID_STR_ERR_DELETERECORDS"
msgid "Error deleting the specified records"
msgstr "Fel vid radering av de angivna dataposterna"

#: forms/inc/strings.hrc:37
msgctxt "RID_STR_NEED_NON_NULL_OBJECT"
msgid "The object cannot be NULL."
msgstr "Objektet får inte vara NULL."

#: forms/inc/strings.hrc:38
msgctxt "RID_STR_OPEN_GRAPHICS"
msgid "Insert Image from..."
msgstr "Infoga bild från..."

#: forms/inc/strings.hrc:39
msgctxt "RID_STR_CLEAR_GRAPHICS"
msgid "Remove Image"
msgstr "Ta bort bild"

#: forms/inc/strings.hrc:40
msgctxt "RID_STR_INVALIDSTREAM"
msgid "The given stream is invalid."
msgstr "Den givna strömmen är ogiltig."

#: forms/inc/strings.hrc:41
msgctxt "RID_STR_SYNTAXERROR"
msgid "Syntax error in query expression"
msgstr "Fel vid analys av frågeuttryck"

#: forms/inc/strings.hrc:42
msgctxt "RID_STR_INCOMPATIBLE_TYPES"
msgid "The value types supported by the binding cannot be used for exchanging data with this control."
msgstr "Värdetyperna som stöds av bindningen kan inte användas för att utbyta data med det här kontrollfältet."

#: forms/inc/strings.hrc:43
msgctxt "RID_STR_LABEL_RECORD"
msgid "Record"
msgstr "Datapost"

#: forms/inc/strings.hrc:44
msgctxt "RID_STR_INVALID_VALIDATOR"
msgid "The control is connected to an external value binding, which at the same time acts as validator. You need to revoke the value binding, before you can set a new validator."
msgstr "Kontrollfältet har en bindning till ett externt värde, som samtidigt fungerar som en validerare. Du måste upphäva värdebindningen innan du kan använda en ny validerare."

#: forms/inc/strings.hrc:45
msgctxt "RID_STR_LABEL_OF"
msgid "of"
msgstr "av"

#: forms/inc/strings.hrc:46
msgctxt "RID_STR_QUERY_SAVE_MODIFIED_ROW"
msgid ""
"The content of the current form has been modified.\n"
"Do you want to save your changes?"
msgstr ""
"Innehållet i det aktuella formuläret har ändrats.\n"
"Vill du spara ändringarna?"

#: forms/inc/strings.hrc:47
msgctxt "RID_STR_COULD_NOT_SET_ORDER"
msgid "Error setting the sort criteria"
msgstr "Det gick inte att ange sorteringskriterier"

#: forms/inc/strings.hrc:48
msgctxt "RID_STR_COULD_NOT_SET_FILTER"
msgid "Error setting the filter criteria"
msgstr "Det gick inte att ange filterkriterier"

#: forms/inc/strings.hrc:49
msgctxt "RID_STR_FEATURE_REQUIRES_PARAMETERS"
msgid "To execute this function, parameters are needed."
msgstr "Om du vill använda den här funktionen måste du ha vissa parametrar."

#: forms/inc/strings.hrc:50
msgctxt "RID_STR_FEATURE_NOT_EXECUTABLE"
msgid "This function cannot be executed, but is only for status queries."
msgstr "Det går inte att använda den här funktionen eftersom den endast är till för statusfrågor."

#: forms/inc/strings.hrc:51
msgctxt "RID_STR_FEATURE_UNKNOWN"
msgid "Unknown function."
msgstr "Funktionen är okänd."

#: forms/inc/strings.hrc:53
msgctxt "RID_STR_XFORMS_NO_BINDING_EXPRESSION"
msgid "Please enter a binding expression."
msgstr "Ange ett bindningsuttryck."

#: forms/inc/strings.hrc:54
msgctxt "RID_STR_XFORMS_INVALID_BINDING_EXPRESSION"
msgid "This is an invalid binding expression."
msgstr "Detta är ett ogiltigt bindningsuttryck."

#: forms/inc/strings.hrc:55
msgctxt "RID_STR_XFORMS_INVALID_VALUE"
msgid "Value is invalid."
msgstr "Värdet är ogiltigt."

#: forms/inc/strings.hrc:56
msgctxt "RID_STR_XFORMS_REQUIRED"
msgid "A value is required."
msgstr "Ett värde krävs."

#: forms/inc/strings.hrc:57
msgctxt "RID_STR_XFORMS_INVALID_CONSTRAINT"
msgid "The constraint '$1' not validated."
msgstr "Begränsningen \"$1\" har inte validerats."

#: forms/inc/strings.hrc:58
msgctxt "RID_STR_XFORMS_VALUE_IS_NOT_A"
msgid "The value is not of the type '$2'."
msgstr "Värdet har inte typen \"$2\"."

#: forms/inc/strings.hrc:59
msgctxt "RID_STR_XFORMS_VALUE_MAX_INCL"
msgid "The value must be smaller than or equal to $2."
msgstr "Värdet måste vara mindre än eller lika med $2."

#: forms/inc/strings.hrc:60
msgctxt "RID_STR_XFORMS_VALUE_MAX_EXCL"
msgid "The value must be smaller than $2."
msgstr "Värdet måste vara mindre än $2."

#: forms/inc/strings.hrc:61
msgctxt "RID_STR_XFORMS_VALUE_MIN_INCL"
msgid "The value must be greater than or equal to $2."
msgstr "Värdet måste vara större än eller lika med $2."

#: forms/inc/strings.hrc:62
msgctxt "RID_STR_XFORMS_VALUE_MIN_EXCL"
msgid "The value must be greater than $2."
msgstr "Värdet måste vara större än $2."

#: forms/inc/strings.hrc:63
msgctxt "RID_STR_XFORMS_VALUE_TOTAL_DIGITS"
msgid "$2 digits allowed at most."
msgstr "Högst $2 siffror får anges."

#: forms/inc/strings.hrc:64
msgctxt "RID_STR_XFORMS_VALUE_FRACTION_DIGITS"
msgid "$2 fraction digits allowed at most."
msgstr "Högst $2 decimaler får anges."

#: forms/inc/strings.hrc:65
msgctxt "RID_STR_XFORMS_VALUE_LENGTH"
msgid "The string must be $2 characters long."
msgstr "Strängen måste innehålla $2 tecken."

#: forms/inc/strings.hrc:66
msgctxt "RID_STR_XFORMS_VALUE_MIN_LENGTH"
msgid "The string must be at least $2 characters long."
msgstr "Strängen måste innehålla minst $2 tecken."

#: forms/inc/strings.hrc:67
msgctxt "RID_STR_XFORMS_VALUE_MAX_LENGTH"
msgid "The string can only be $2 characters long at most."
msgstr "Strängen får innehålla högst $2 tecken."

