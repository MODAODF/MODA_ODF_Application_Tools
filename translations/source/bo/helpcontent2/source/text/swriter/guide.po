msgstr "<variable id=\"indices_toc\"><link href=\"text/swriter/guide/indices_toc.xhp\" name=\"དཀར་ཆག་གསར་འཛུགས་\">དཀར་ཆག་གསར་འཛུགས་</link></variable>"

