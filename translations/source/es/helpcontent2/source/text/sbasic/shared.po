msgctxt ""
"03110100.xhp\n"
"par_id3149177\n"
"help.text"
msgid "Result = Expression1 { = | < | > | <= | >= } Expression2"
msgstr "Resultado = Expresión1 { = | < | > | <= | >= } Expresión2"

